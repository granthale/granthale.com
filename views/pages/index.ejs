<% var navbarColor = '#FDFDBD'; %>
<body style="background-color: <%= navbarColor %>">

  <!--——————————————————————————————————————————————-->
  <!-- ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​ ​-->
  <!-- ​ ​ ​ ​ ​We meet each other on this road, ​ ​ ​ ​ ​ ​ ​ ​ ​-->
