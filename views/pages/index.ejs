<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Grant Hale</title>
</head>
<body>
  <%- include('../partials/navbar.ejs') %>
  <div class="px-4 py-5 my-5 text-center">
    <!-- <img class="d-block mx-auto mb-4" src="public/value.png" alt="" width="72" height="57"> -->
    <!-- Element to contain animated typing -->
    <span id="element"></span>

    <!-- Load library from the CDN -->
    <script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>

    <!-- Setup and start animation! -->
    <script>
      var typed = new Typed('#element', {
        strings: ['<h1 class="display-5 fw-bold text-body-emphasis">Welcome, my name is Grant.</h1>', '<h1 class="display-5 fw-bold text-body-emphasis">Feel free to explore!</h1>'],
        typeSpeed: 60,
        showCursor: false,
        backSpeed: 30
      });
    </script>

    <!-- <h1 class="display-5 fw-bold text-body-emphasis">Welcome!</h1> -->
  
    <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">My name is Grant and I'm 22 years old. I love to grapple with challenging and interesting ideas - especially those in computer science. </p>
  </div>
    <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
        <a href="/about"> <button type="button" class="btn btn-primary btn-lg px-4 gap-3">Who am I?</button>
        </a>
        <a href="/about"><button type="button" class="btn btn-outline-primary btn-lg px-4">What Do I Believe?</button>
        </a>
        <a href="/writing/essays"><button type="button" class="btn btn-outline-primary btn-lg px-4">What Am I Thinking About?</button>
        </a>
    </div>
  </div>
  <script>
    var typed = new Typed('#typed', {
      stringsElement: '#typed-strings'
    });
  </script>

</body>
</html>